<?xml version="1.0" encoding="utf-8"?>
<form>



	<fieldset name="details" label="COM_DUES_DUES_DETAILS">

		<field
			name="@text_details"
			type="note"
			label=""
			description="COM_DUES_EDIT_DETAILS"
		/>

		<field
			name="user_id"
			type="user"
			label="COM_DUES_FIELD_LINKED_USER_LABEL"
			description="COM_DUES_FIELD_LINKED_USER_DESC"
		/>
		<field
			name="year"
			type="list"
			label="COM_DUES_FIELD_YEAR"
			description="COM_DUES_FIELD_YEAR_DESC"
			class="chzn-color"
			>
			<option value="2017">2017</option>
			<option value="2018">2018</option>
			<option value="2019">2019</option>
		</field>
		<field
			name="status"
			type="list"
			label="COM_DUES_FIELD_SHOW_STATUS_LABEL"
			description="COM_DUES_FIELD_SHOW_STATUS_DESC"
			class="chzn-color"
			>
			<option value="0">Owed</option>
			<option value="1">Paid</option>
		</field>
		

		<field
			name="date_paid"
			type="text"
			label="COM_DUES_FIELD_INFORMATION_SUBURB_LABEL"
			description="COM_DUES_FIELD_INFORMATION_SUBURB_DESC"
			size="30"
		/>
		<field
			name="published"
			type="list"
			label="JSTATUS"
			description="JFIELD_PUBLISHED_DESC"
			default="1"
			id="published"
			class="chzn-color-state"
			size="1"
			>
			<option value="1">JPUBLISHED</option>
			<option value="0">JUNPUBLISHED</option>
			<option value="2">JARCHIVED</option>
			<option value="-2">JTRASHED</option>

		</field>
	</fieldset>


</form>
